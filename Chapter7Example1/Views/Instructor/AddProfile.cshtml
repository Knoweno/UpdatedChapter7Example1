@model Chapter7Example1.Models.Instructor


<h2>Add Your Profile</h2>
<form asp-action="AddProfile" method="post">
    
    <input asp-for="@Model.UserId" type="hidden" />
    <label asp-for="InstructorName"></label>
    <input asp-for="InstructorName"/>
    <br />

    <label asp-for="OfficeLocation"></label>
    <input asp-for="OfficeLocation" />
    <br />

    <button type="submit">Add Profile</button>
    </form>
    <p><a asp-action="Index">Back to Instructor home</a></p>